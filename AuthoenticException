package shiroDemo;

import org.apache.shiro.authc.AuthenticationException;

public class AuthoenticException extends AuthenticationException {
	private static final long serialVersionUID = 1L;

public AuthoenticException(String message) {
	 super(message);
}

public AuthoenticException(String message, Throwable cause) {
    super(message, cause);
}

public AuthoenticException(Throwable cause) {
    super(cause);
}


}
